{
  "name": "vsc-quokka-sidebar",
  "displayName": "vsc_quokka_sidebar",
  "description": "Adds buttons to the status bar in VSC to control Quokka.",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "sketchbuch",
  "repository": {
    "type": "git",
    "url": "https://github.com/sketchbuch/vsc_quokka_statusbar"
  },
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "extensionDependency": [
    "wallabyjs.quokka-vscode"
  ]
}
